  eprinttype   = {pmid},
  file         = {/Volumes/GoogleDrive/My Drive/Zotero_papers/Goodwin et al_2015_Oxford Nanopore sequencing, hybrid error correction, and de novo assembly of a.pdf;/Users/lucblassel/Zotero/storage/S8HHBWK4/1750.html},
  issn         = {1088-9051, 1549-5469},
  journaltitle = {Genome Research},
  langid       = {english},
  number       = {11},
  pages        = {1750--1756},
  publisher    = {{Cold Spring Harbor Lab}},
  shortjournal = {Genome Res.},
  title        = {Oxford {{Nanopore}} Sequencing, Hybrid Error Correction, and de Novo Assembly of a Eukaryotic Genome},
