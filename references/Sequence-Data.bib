  file         = {/Users/lucblassel/Library/CloudStorage/GoogleDrive-luc.blassel@gmail.com/My Drive/Zotero_papers/Srivathsan et al_2018_A MinION™-based pipeline for fast and cost-effective DNA barcoding.pdf;/Users/lucblassel/Zotero/storage/SXC7BSXA/1755-0998.html},
  issn         = {1755-0998},
  journaltitle = {Molecular Ecology Resources},
  keywords     = {amplicon sequencing,DNA barcoding,nanopore sequencing,next-generation sequencing barcoding},
  langid       = {english},
