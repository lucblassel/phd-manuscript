  doi          = {10.1007/s13353-011-0057-x},
  file         = {/Users/lucblassel/Zotero/storage/FCYXKZNE/Pareek et al. - 2011 - Sequencing technologies and genome sequencing.pdf},
  issn         = {2190-3883},
  journaltitle = {Journal of Applied Genetics},
  keywords     = {CHIP-chip,Chip-seq,De novo assembling,High-throughput next generation sequencing,Personal genomics,Re-sequencing,RNA-seq},
  langid       = {english},
  number       = {4},
  pages        = {413--435},
  shortjournal = {J Appl Genetics},
  title        = {Sequencing Technologies and Genome Sequencing},
  url          = {https://doi.org/10.1007/s13353-011-0057-x},
