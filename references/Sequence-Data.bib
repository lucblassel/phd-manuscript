  issn         = {2166-2746},
  journaltitle = {Journal of Vacuum Science \& Technology B},
  number       = {6},
  pages        = {061809},
  publisher    = {{American Vacuum Society}},
  title        = {Detection of {{DNA}} Homopolymer with Graphene Nanopore},
  url          = {https://avs.scitation.org/doi/full/10.1116/1.5116295},
