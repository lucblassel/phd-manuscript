  file         = {/Users/lucblassel/Library/CloudStorage/GoogleDrive-luc.blassel@gmail.com/My Drive/Zotero_papers/Murigneux et al_2020_Comparison of long-read methods for sequencing and assembly of a plant genome.pdf;/Users/lucblassel/Zotero/storage/UJDJEG26/6042729.html},
  issn         = {2047-217X},
  journaltitle = {GigaScience},
  number       = {12},
