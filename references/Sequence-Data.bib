  number       = {1},
  pages        = {171--175},
  shortjournal = {Analytical Biochemistry},
  title        = {Solid {{Phase DNA Minisequencing}} by an {{Enzymatic Luminometric Inorganic Pyrophosphate Detection Assay}}},
