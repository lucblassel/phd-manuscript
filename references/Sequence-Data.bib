  issn         = {1471-2105},
  journaltitle = {BMC bioinformatics},
  keywords     = {Amino Acid Substitution,Animals,Base Sequence,Biological Evolution,Chickens,Codon,Computational Biology,Computer Simulation,Evolution; Molecular,Humans,Likelihood Functions,Mice,Models; Genetic,Models; Statistical,Mutation,Phylogeny,Sequence Alignment,Sequence Analysis; DNA,Software,Species Specificity,Xenopus,Zebrafish},
