  file         = {/Users/lucblassel/Library/CloudStorage/GoogleDrive-luc.blassel@gmail.com/My Drive/Zotero_papers/Nyren et al_1993_Solid Phase DNA Minisequencing by an Enzymatic Luminometric Inorganic.pdf;/Users/lucblassel/Zotero/storage/5SWLG7ZA/S0003269783710249.html},
  issn         = {0003-2697},
  journaltitle = {Analytical Biochemistry},
