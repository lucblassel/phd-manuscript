  journaltitle = {IEEE Communications Letters},
  keywords     = {3G mobile communication,Complexity theory,constrained code,DNA,DNA data storage,long term data storage,Memory,Precoding,run-length limited code,Silicon},
  number       = {6},
  pages        = {963--966},
  title        = {Construction of {{Bio-Constrained Code}} for {{DNA Data Storage}}},
  volume       = {23}
}

@article{wangNanoporeSequencingTechnology2021,
