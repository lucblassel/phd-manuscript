  issn         = {1545-9985},
  journal      = {Nature Structural Biology},
  month        = {Feb},
  number       = {2},
  pages        = {171--178},
  shortjournal = {Nat. Struct. Biol},
  title        = {Sequencespace: A Tool for Family Analysis},
