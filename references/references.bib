  issn         = {1468-1293},
  journaltitle = {HIV Medicine},
  keywords     = {cohort study,epidemiology,HIV,natural history,observational database},
  langid       = {english},
  number       = {2},
  pages        = {115--124},
  shorttitle   = {The Creation of a Large {UK-based} Multicentre Cohort of {HIV-infected} Individuals},
  title        = {The Creation of a Large {UK-based} Multicentre Cohort of {HIV-infected} Individuals: {The UK Collaborative HIV Cohort} ({UK CHIC}) {Study}},
  url          = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1468-1293.2004.00197.x},
  urldate      = {2019-09-05},
