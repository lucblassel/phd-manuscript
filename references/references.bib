  number       = {15},
  pages        = {1909--1922},
  shortjournal = {Current Pharmaceutical Design},
  title        = {Recent {Progress} in the {Design} of {Small Molecule Inhibitors} of {HIV RNase H}},
  volume       = {12}
}

