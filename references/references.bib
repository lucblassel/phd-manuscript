  issn         = {1367-4803},
  journaltitle = {Bioinformatics},
  number       = {3},
  pages        = {440--445},
  shortjournal = {Bioinformatics},
  shorttitle   = {{{PatternHunter}}},
  title        = {{{PatternHunter}}: Faster and More Sensitive Homology Search},
