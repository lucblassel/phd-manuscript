  eprint       = {17961103},
  eprinttype   = {pmid},
  issn         = {0889-2229},
  journaltitle = {AIDS research and human retroviruses},
  keywords     = {Cohort Studies,Female,HIV Infections,HIV-1,HIV-2,Humans,Incidence,Longitudinal Studies,Molecular Epidemiology,Molecular Sequence Data,Phylogeny,Population Surveillance,Prevalence,Senegal,Sex Work},
  langid       = {english},
  number       = {10},
  pages        = {1189--1196},
  shortjournal = {AIDS Res Hum Retroviruses},
  shorttitle   = {Twenty Years of Prospective Molecular Epidemiology in {{Senegal}}},
  title        = {Twenty Years of Prospective Molecular Epidemiology in {{Senegal}}: Changes in {{HIV}} Diversity},
