  shortjournal = {Joss},
  shorttitle   = {Nextclade},
  title        = {Nextclade: Clade Assignment, Mutation Calling and Quality Control for Viral Genomes},
  url          = {https://joss.theoj.org/papers/10.21105/joss.03773},
  urldate      = {2022-06-16},
  volume       = {6}
}

