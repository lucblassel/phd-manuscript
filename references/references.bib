  ids       = {almogyCostefficientWholeGenomesequencing2022},
  langid    = {english},
  pages     = {2022.05.29.493900},
  publisher = {{bioRxiv}},
  title     = {Cost-Efficient Whole Genome-Sequencing Using Novel Mostly Natural Sequencing-by-Synthesis Chemistry and Open Fluidics Platform},
