  number       = {8},
  options      = {useprefix=true},
  pages        = {1203--1212},
  title        = {Prevalence of Transmitted Drug Resistance Associated Mutations and {{HIV-1}} Subtypes in New {{HIV-1}} Diagnoses, {{U}}.{{S}}.–2006},
