  location  = {{Berlin, Heidelberg}},
  pages     = {39--53},
  publisher = {{Springer}},
  title     = {Effects of {{Ultraviolet Radiation}} on {{DNA}}},
  url       = {https://doi.org/10.1007/978-3-540-71414-9_3},
