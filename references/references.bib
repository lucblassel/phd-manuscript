  shorttitle   = {Polypolish},
  title        = {Polypolish: {{Short-read}} Polishing of Long-Read Bacterial Genome Assemblies},
  url          = {https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1009802},
  urldate      = {2022-06-07},
  volume       = {18}
}
