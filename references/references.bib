  shortjournal = {Genome Res.},
  shorttitle   = {The {{Bioperl Toolkit}}},
  title        = {The {{Bioperl Toolkit}}: {{Perl Modules}} for the {{Life Sciences}}},
  url          = {https://genome.cshlp.org/content/12/10/1611},
  urldate      = {2022-06-14},
