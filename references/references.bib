  issn         = {1525-4135},
  journaltitle = {JAIDS Journal of Acquired Immune Deficiency Syndromes},
  journaltitle = {JAIDS Journal of Acquired Immune Deficiency Syndromes},
  langid       = {american},
  language     = {en-US},
  month        = {April},
  number       = {4},
  pages        = {439--444},
  title        = {Comparison of the {Precision} and {Sensitivity} of the {Antivirogram} and {PhenoSense HIV Drug Susceptibility Assays}},
  url          = {https://doi.org/10.1097/01.qai.0000147526.64863.53},
  urldate      = {2021-05-18},
