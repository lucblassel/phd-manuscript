  issn         = {1474-1741},
  issue        = {6},
  journaltitle = {Nature Reviews Immunology},
  keywords     = {Biomedicine,general,Immunology},
  langid       = {english},
  number       = {6},
  pages        = {447--457},
  publisher    = {{Nature Publishing Group}},
  shortjournal = {Nat Rev Immunol},
  shorttitle   = {Setting the Stage},
  title        = {Setting the Stage: Host Invasion by {{HIV}}},
  url          = {https://www.nature.com/articles/nri2302},
  urldate      = {2022-06-20},
  volume       = {8}
}
