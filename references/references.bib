  journaltitle = {Science},
  keywords     = {Amino Acid Sequence,Base Sequence,Codon,DNA; Viral,Genes; Viral,HIV,Molecular Sequence Data,Molecular Weight,RNA; Messenger,Viral Envelope Proteins,Viral Proteins},
  langid       = {english},
  number       = {4846},
