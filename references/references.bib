  pages        = {705--710},
  publisher    = {{Nature Publishing Group}},
  shortjournal = {Nat Methods},
  title        = {Long-Read Mapping to Repetitive Reference Sequences Using {{Winnowmap2}}},
  url          = {https://www.nature.com/articles/s41592-022-01457-8},
  urldate      = {2022-06-15},
  volume       = {19}
}
