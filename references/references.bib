  number       = {14},
  pages        = {2499--2502},
  shortjournal = {Bioinformatics},
  shorttitle   = {{{iFeature}}},
  title        = {{{iFeature}}: A {{Python}} Package and Web Server for Features Extraction and Selection from Protein and Peptide Sequences},
