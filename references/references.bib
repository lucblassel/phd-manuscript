  keywords     = {Allosteric Regulation,Amino Acid Sequence,Amino Acid Substitution,Conserved Sequence,Dimerization,Drug Resistance; Microbial,HIV Reverse Transcriptase,HIV-1,Humans,Lamivudine,Macromolecular Substances,Models; Molecular,Molecular Sequence Data,Nucleic Acid Conformation,Point Mutation,Protein Conformation,RNA; Viral,Templates; Genetic},
  langid       = {english},
  number       = {18},
