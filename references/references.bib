  issn         = {0066-4804, 1098-6596},
  journaltitle = {Antimicrobial Agents and Chemotherapy},
  langid       = {english},
  number       = {9},
  pages        = {3067--3074},
  title        = {Predictive {{Genotypic Algorithm}} for {{Virologic Response}} to {{Lopinavir-Ritonavir}} in {{Protease Inhibitor-Experienced Patients}}},
  url          = {https://aac.asm.org/content/51/9/3067},
