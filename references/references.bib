  number        = {arXiv:2206.13517},
  primaryclass  = {cs, q-bio},
  publisher     = {{arXiv}},
  shorttitle    = {{ProGen2}},
  title         = {{ProGen2}: {Exploring} the {Boundaries} of {Protein Language Models}},
  url           = {http://arxiv.org/abs/2206.13517},
