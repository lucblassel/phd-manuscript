  eprint       = {33640492},
  eprinttype   = {pmid},
  issn         = {1535-9476},
  journaltitle = {Molecular \& Cellular Proteomics : MCP},
  pages        = {100062},
  pmcid        = {Pmc8058560},
  shortjournal = {Mol Cell Proteomics},
  title        = {Reflections on the {HUPO Human Proteome Project}, the {Flagship Project} of the {Human Proteome Organization}, at 10\~{Years}},
