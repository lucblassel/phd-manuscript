  date         = {2017-12-01},
  doi          = {10.1093/infdis/jix397},
  issn         = {0022-1899},
  issue        = {suppl\_9},
  journaltitle = {The Journal of Infectious Diseases},
  pages        = {S829-s833},
  shortjournal = {The Journal of Infectious Diseases},
  title        = {Next-{{Generation Human Immunodeficiency Virus Sequencing}} for {{Patient Management}} and {{Drug Resistance Surveillance}}},
