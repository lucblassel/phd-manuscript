  isbn      = {978-3-540-78983-3},
  keywords  = {association rule,interestingness measure,Ontology alignment,ontology matching},
  langid    = {english},
  location  = {{Berlin, Heidelberg}},
  pages     = {227--245},
  publisher = {{Springer Berlin Heidelberg}},
  series    = {Studies in {{Computational Intelligence}}},
  title     = {On the Use of {{Implication Intensity}} for Matching Ontologies and Textual Taxonomies},
  url       = {https://doi.org/10.1007/978-3-540-78983-3_11},
  urldate   = {2019-09-26}
