  shortjournal = {Journal of Molecular Biology},
  title        = {Predicting the Secondary Structure of Globular Proteins Using Neural Network Models},
  url          = {https://www.sciencedirect.com/science/article/pii/0022283688905645},
  urldate      = {2022-09-12},
