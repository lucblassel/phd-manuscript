  issn         = {1546-1696},
  issue        = {9},
  journaltitle = {Nature Biotechnology},
  keywords     = {Genome assembly algorithms},
  langid       = {english},
  number       = {9},
  pages        = {1044--1053},
  publisher    = {{Nature Publishing Group}},
  shortjournal = {Nat Biotechnol},
  title        = {Nanopore Sequencing and the {Shasta} Toolkit Enable Efficient de Novo Assembly of Eleven Human Genomes},
  url          = {https://www.nature.com/articles/s41587-020-0503-6},
  urldate      = {2022-06-09},
