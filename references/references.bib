  publisher  = {{Association for Computing Machinery}},
  series     = {{{Kdd}} '11},
  shorttitle = {Leakage in Data Mining},
  title      = {Leakage in Data Mining: Formulation, Detection, and Avoidance},
  url        = {https://doi.org/10.1145/2020408.2020496},
  urldate    = {2022-09-19}
}
