  doi          = {10.1186/1742-4690-11-7},
  issn         = {1742-4690},
  journaltitle = {Retrovirology},
  number       = {1},
  pages        = {7},
  shortjournal = {Retrovirology},
  title        = {The {{M50I}} Polymorphic Substitution in Association with the {{R263K}} Mutation in {{HIV-1}} Subtype {{B}} Integrase Increases Drug Resistance but Does Not Restore Viral Replicative Fitness},
