  publisher    = {{Nature Publishing Group}},
  shortjournal = {Nat Rev Microbiol},
  shorttitle   = {Viruses of the {{Archaea}}},
  title        = {Viruses of the {{Archaea}}: A Unifying View},
  url          = {https://www.nature.com/articles/nrmicro1527},
