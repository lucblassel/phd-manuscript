  langid       = {american},
  language     = {en-US},
  month        = nov,
  number       = {17},
  pages        = {2577},
  title        = {In-Depth Analysis of {{HIV}}-1 Drug Resistance Mutations in {{HIV}}-Infected Individuals Failing First-Line Regimens in {{West}} and {{Central Africa}}},
  url          = {https://journals.lww.com/aidsonline/Fulltext/2016/11130/In_depth_analysis_of_HIV_1_drug_resistance.3.aspx},
  urldate      = {2018-09-20},
