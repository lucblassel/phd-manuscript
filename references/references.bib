  journaltitle = {Statistics in Medicine},
  langid       = {english},
  number       = {22},
  pages        = {2511--2528},
  shorttitle   = {Methods Correcting for Multiple Testing},
  title        = {Methods Correcting for Multiple Testing: Operating Characteristics},
  url          = {https://onlinelibrary.wiley.com/doi/abs/10.1002/%28SICI%291097-0258%2819971130%2916%3A22%3C2511%3A%3AAID-SIM693%3E3.0.CO%3B2-4},
