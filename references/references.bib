  eprinttype   = {pmid},
  issn         = {1083-351x},
  journaltitle = {The Journal of Biological Chemistry},
  keywords     = {228R,Adenosine Triphosphate,Anti-HIV Agents,DNA Polymerase,DNA; Viral,Drug Action,Drug Resistance,Enzyme Stability,Foscarnet,HEK293 Cells,HIV Reverse Transcriptase,HIV-1,Humans,Indoles,Nitriles,Protein Binding,Pyridones,Reverse Transcription,Structure-Activity Relationship},
  langid       = {english},
  number       = {24},
  options      = {useprefix=true},
  pages        = {17336--17346},
  pmcid        = {Pmc3682535},
  shortjournal = {J. Biol. Chem.},
  title        = {Formation of a Quaternary Complex of {{HIV-1}} Reverse Transcriptase with a Nucleotide-Competing Inhibitor and Its {{ATP}} Enhancer},
