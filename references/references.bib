  options   = {useprefix=true},
  pages     = {94--103},
  publisher = {{Springer Berlin Heidelberg}},
  series    = {Lecture {{Notes}} in {{Computer Science}}},
  title     = {Identification of {{Diatoms}} by {{Grid Graph Matching}}}
}
