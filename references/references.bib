  journaltitle = {Plos One},
  keywords     = {Bacterial genomics,Genomics,Mycobacterium tuberculosis,Nucleotides,Pneumococcus,Sequence alignment,Single nucleotide polymorphisms,Tandem repeats},
  langid       = {english},
  number       = {11},
  pages        = {e112963},
  publisher    = {{Public Library of Science}},
  shortjournal = {Plos One},
  shorttitle   = {Pilon},
  title        = {Pilon: {{An Integrated Tool}} for {{Comprehensive Microbial Variant Detection}} and {{Genome Assembly Improvement}}},
  url          = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0112963},
  urldate      = {2022-06-09},
  volume       = {9}
}
