}

@article{esnoufUniqueFeaturesStructure1997,
  abstract     = {The viral reverse transcriptase (RT) provides an attractive target in the search for anti-HIV therapies. The nonnucleoside inhibitors (NNIs) are a diverse set of compounds (usually HIV-1 specific) that function by distorting the polymerase active site upon binding in a nearby pocket. Despite being potent and of generally low toxicity, their clinical use has been limited by rapid selection for resistant viral populations. The 2.65-A resolution structure of the complex between HIV-1 RT and the bis(heteroaryl)piperazine (BHAP) NNI, 1-(5-methanesulfonamido-1H-indol-2-yl-carbonyl)-4- [3-(1-methyl-ethylamino) pyridinyl] piperazine (U-90152), reveals the inhibitor conformation and bound water molecules. The bulky U-90152 molecule occupies the same pocket as other NNIs, but the complex is stabilized quite differently, in particular by hydrogen bonding to the main chain of Lys-103 and extensive hydrophobic contacts with Pro-236. These interactions rationalize observed resistance mutations, notably Pro-236-Leu, which occurs characteristically for BHAPs. When bound, part of U-90152 protrudes into the solvent creating a channel between Pro-236 and the polypeptide segments 225-226 and 105-106, giving the first clear evidence of the entry mode for NNIs. The structure allows prediction of binding modes for related inhibitors [(altrylamino)piperidine-BHAPs] and suggests changes to U-90152, such as the addition of a 6 amino group to the pyridine ring, which may make binding more resilient to mutations in the RT. The observation of novel hydrogen bonding to the protein main chain may provide lessons for the improvement of quite different inhibitors.},
  author       = {Esnouf, R. M. and Ren, J. and Hopkins, A. L. and Ross, C. K. and Jones, E. Y. and Stammers, D. K. and Stuart, D. I.},
  date         = {1997-04-15},
  doi          = {10.1073/pnas.94.8.3984},
  eprint       = {9108091},
  eprinttype   = {pmid},
  issn         = {0027-8424},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  keywords     = {Acquired Immunodeficiency Syndrome,Anti-HIV Agents,Delavirdine,Drug Resistance; Microbial,HIV Reverse Transcriptase,HIV-1,Humans,Indoles,Molecular Sequence Data,Mutation,Piperazines,Reverse Transcriptase Inhibitors},
  langid       = {english},
  number       = {8},
  pages        = {3984--3989},
  pmcid        = {Pmc20554},
  shortjournal = {Proc Natl Acad Sci U S A},
  title        = {Unique Features in the Structure of the Complex between {HIV-1} Reverse Transcriptase and the Bis(Heteroaryl)Piperazine ({BHAP}) {U-90152} Explain Resistance Mutations for This Nonnucleoside Inhibitor},
  url          = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC20554/},
  urldate      = {2022-06-20},
  volume       = {94}
}

@incollection{espositoHIVIntegraseStructure1999,
  abstract  = {This chapter discusses various human immunodeficiency virus (HIV) integrase structure and function. HIV integrase consists of three domains, the structures of that are individually determined by X-ray crystallography or nuclear magnetic resonance (NMR) spectroscopy. The chapter discusses the domain structure of HIV-type 1 (HIV-1) Integrase. The core domain, spanning residues 50–212, is responsible for the catalytic activity of the enzyme. The crystal structure of a dimer of this domain shows similarity to other proteins that carry out polynucleotidyl transfer, including MuA transposase and Ribonuclease H (RNase H). The small N-terminal domain folds into a dimeric helix-turn-helix structure, which is stabilized by the coordination of zinc with conserved histidine (His) and cysteine (Cys) residues. The chapter also discusses the catalytic core domain of the HIV integrase enzymes. In this reaction, a branched DNA substrate mimicking the product of DNA strand transfer is resolved into its viral DNA and target DNA parts. There is no evidence that disintegration is relevant in vivo, but it is a useful assay of the ability of integrase to carry out its basic function of catalyzing polynucleotidyl transfer. The chapter concludes with the multimeric organization of HIV Integrase and HIV Integrase–DNA Interactions.},
  author    = {Esposito, Dominic and Craigie, Robert},
  booktitle = {Advances in {Virus Research}},
  date      = {1999-01-01},
  doi       = {10.1016/s0065-3527(08)60304-8},
  editor    = {Rlaramorosch, Karl and Murphy, Frederick A. and Shawn, Aaron J.},
  ids       = {espositoHIVIntegraseStructure1999a},
  langid    = {english},
  pages     = {319--333},
  publisher = {{Academic Press}},
  title     = {{HIV Integrase Structure} and {Function}},
