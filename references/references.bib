  langid       = {english},
  number       = {41},
  pages        = {11537--11542},
  publisher    = {{National Academy of Sciences}},
  shortjournal = {Pnas},
  title        = {Concomitant Emergence of the Antisense Protein Gene of {{HIV-1}} and of the Pandemic},
