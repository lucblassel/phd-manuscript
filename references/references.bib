  eprint       = {11044070},
  eprinttype   = {pmid},
  issn         = {0022-538x},
  journaltitle = {Journal of Virology},
  journaltitle = {Journal of Virology},
  keywords     = {plos\_latex\_template},
  month        = {November},
  number       = {22},
  pages        = {10269--10273},
  pmcid        = {Pmc110900},
  pmid         = {11044070},
  shortjournal = {J Virol},
  title        = {Reduced {Susceptibility} of {Human Immunodeficiency Virus Type} 1 ({HIV}-1) from {Patients} with {Primary HIV Infection} to {Nonnucleoside Reverse Transcriptase Inhibitors}  {Is Associated} with {Variation} at  {Novel Amino Acid Sites}},
  url          = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC110900/},
