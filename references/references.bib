  pmcid        = {Pmc2573403},
  shortjournal = {Aids},
  title        = {Extended Spectrum of {{HIV-1}} Reverse Transcriptase Mutations in Patients Receiving Multiple Nucleoside Analog Inhibitors},
  url          = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2573403/},
  urldate      = {2022-08-18},
